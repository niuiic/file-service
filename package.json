{
  "name": "nest-app",
  "version": "0.1.0",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run -c ./vitest.config.e2e.mts",
    "test:coverage": "vitest run --coverage",
    "build": "nest build",
    "db:store": "drizzle-kit pull",
    "db:restore": "drizzle-kit push",
    "debug": "nest start"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.5",
    "@nestjs/core": "^10.4.5",
    "@nestjs/platform-fastify": "^10.4.5",
    "drizzle-orm": "^0.35.2",
    "ioredis": "^5.4.1",
    "nestjs-minio": "^2.6.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.5",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.36",
    "@types/node": "^22.7.7",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.3",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.13.0",
    "supertest": "^7.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.3"
  }
}
