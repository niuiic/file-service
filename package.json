    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.22",
    "@types/eslint__js": "^8.42.3",
