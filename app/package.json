{
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run -c ./vitest.config.e2e.mts",
    "test:coverage": "vitest run --coverage",
    "build": "tsx build.ts",
    "db:store": "drizzle-kit pull",
    "db:restore": "drizzle-kit push",
    "debug": "nest start"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/core": "^10.4.6",
    "@nestjs/platform-fastify": "^10.4.6",
    "drizzle-orm": "^0.36.0",
    "ioredis": "^5.4.1",
    "minio": "^8.0.2",
    "nestjs-minio": "^2.6.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8",
    "snowflake-id": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@swc/cli": "0.5.0",
    "@swc/core": "^1.8.0",
    "@types/node": "^22.8.7",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.4",
    "drizzle-kit": "^0.27.1",
    "eslint": "^9.14.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.4"
  }
}
