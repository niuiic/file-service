{
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run -c ./vitest.config.e2e.mts",
    "test:coverage": "vitest run --coverage",
    "build": "tsx build.ts",
    "db:store": "drizzle-kit pull",
    "db:restore": "drizzle-kit push",
    "debug": "nest start"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.12",
    "@nestjs/platform-fastify": "^10.4.12",
    "drizzle-orm": "^0.36.4",
    "ioredis": "^5.4.1",
    "json-bigint": "^1.0.0",
    "minio": "^8.0.2",
    "nestjs-minio": "^2.6.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "snowflake-id": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.12",
    "@swc/cli": "0.5.2",
    "@swc/core": "^1.9.3",
    "@types/json-bigint": "^1.0.4",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.8",
    "drizzle-kit": "^0.28.1",
    "supertest": "^7.0.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.8"
  }
}
