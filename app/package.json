{
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "dev": "nest start --watch",
    "dev:mock": "MODE=mock nest start --watch",
    "debug": "MODE=mock nest start --debug",
    "build": "tsx build.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:store": "drizzle-kit pull",
